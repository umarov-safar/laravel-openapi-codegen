{
    "name": "laravel-openapi/codegen",
    "description": "Generate laravel code from openapi specification.",
    "type": "library",
    "require-dev": {
        "orchestra/testbench": "8.x-dev",
        "friendsofphp/php-cs-fixer": "dev-master",
        "laravel/pint": "dev-main"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LaravelOpenapi\\Codegen\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Safar Umarov",
            "email": "safarumarov711@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/",
            "LaravelOpenapi\\Codegen\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve"
        ],
        "lint": [
            "@php vendor/bin/phpstan analyse"
        ],
        "test": "@php vendor/bin/phpunit tests"
    },
    "require": {
        "devizzent/cebe-php-openapi": "1.6.x-dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Openapi\\ServerGenerator\\OpenapiServerGeneratorProvider"
            ]
        }
    }
}
